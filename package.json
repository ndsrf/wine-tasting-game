{
  "name": "wine-tasting-game",
  "version": "1.4.4",
  "description": "Multiplayer wine tasting game application",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "dev:next": "next dev",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "start:next": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate",
    "preversion": "npm install --ignore-scripts",
    "postversion": "npm run changelog && git add CHANGELOG.md && git commit --amend --no-edit",
    "version:patch": "npm version patch -m 'chore: bump version to %s'",
    "version:minor": "npm version minor -m 'chore: bump version to %s'",
    "version:major": "npm version major -m 'chore: bump version to %s'",
    "version:patch:push": "npm run version:patch && git push --follow-tags",
    "version:minor:push": "npm run version:minor && git push --follow-tags",
    "version:major:push": "npm run version:major && git push --follow-tags",
    "version:current": "echo $npm_package_version",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:all": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@google/generative-ai": "^0.24.1",
    "@hookform/resolvers": "3.9.0",
    "@prisma/client": "5.17.0",
    "@react-oauth/google": "^0.12.2",
    "bcryptjs": "2.4.3",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "dotenv": "^17.2.3",
    "i18next": "23.15.1",
    "i18next-browser-languagedetector": "8.0.0",
    "i18next-resources-to-backend": "1.2.1",
    "jsonwebtoken": "9.0.2",
    "lucide-react": "0.427.0",
    "next": "14.2.5",
    "next-auth": "4.24.7",
    "openai": "4.56.0",
    "qrcode.react": "^4.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.52.1",
    "react-i18next": "15.0.1",
    "redis": "4.7.0",
    "socket.io": "4.7.5",
    "socket.io-client": "4.7.5",
    "tailwind-merge": "2.5.2",
    "typescript": "5.5.4",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@types/bcryptjs": "2.4.6",
    "@types/jsonwebtoken": "9.0.6",
    "@types/node": "22.4.1",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "8.2.0",
    "@typescript-eslint/parser": "8.2.0",
    "autoprefixer": "10.4.20",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "husky": "^9.0.0",
    "postcss": "8.4.41",
    "prisma": "5.17.0",
    "tailwindcss": "3.4.9"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
